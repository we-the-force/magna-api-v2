{
  "routes": [
    {
      "method": "GET",
      "path": "/",
      "handler": "import-content.index",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "DELETE",
      "path": "/:importId",
      "handler": "import-content.delete",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "POST",
      "path": "/:importId/undo",
      "handler": "import-content.undo",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
          "method": "POST",
          "path": "/",
          "handler": "import-content.create",
          "config": {
            "policies": ["isAdmin"]
          }
    },
    {
      "method": "GET",
      "path": "/importconfig",
      "handler": "ImportConfig.find",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "GET",
      "path": "/importconfig/:importId",
      "handler": "ImportConfig.findOne",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "GET",
      "path": "/importconfig/count",
      "handler": "ImportConfig.count",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "PUT",
      "path": "/importconfig/:importId",
      "handler": "ImportConfig.update",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "DELETE",
      "path": "/importconfig/:importId",
      "handler": "ImportConfig.delete",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "GET",
      "path": "/importeditem",
      "handler": "ImportedItem.find",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "GET",
      "path": "/importeditem/:importId",
      "handler": "ImportedItem.findOne",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "GET",
      "path": "/importeditem/count",
      "handler": "ImportedItem.count",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "PUT",
      "path": "/importeditem/:importId",
      "handler": "ImportedItem.update",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "DELETE",
      "path": "/importeditem/:importId",
      "handler": "ImportedItem.delete",
      "config": {
        "policies": ["isAdmin"]
      }
    }
  ]
}
